{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true,
        "jest/globals": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "plugins": [
        "jest"
    ],
    "rules": {
        "semi": [
            "error",
            "never"
        ],
        "no-unused-vars": "off",
        "consistent-return": "off",
        "no-underscore-dangle": "off",
        "object-curly-newline": "off"
    }
}